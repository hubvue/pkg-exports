{
  "name": "pkg-exports",
  "type": "module",
  "version": "0.0.2",
  "packageManager": "pnpm@7.8.0",
  "description": "",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/antfu/pkg-exports#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/pkg-exports.git"
  },
  "bugs": "https://github.com/antfu/pkg-exports/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "local-pkg": "^0.4.2",
    "mlly": "^0.5.7",
    "tinypool": "^0.2.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.2",
    "@antfu/ni": "^0.17.2",
    "@antfu/utils": "^0.5.2",
    "@types/node": "^18.6.3",
    "axios": "^0.27.2",
    "bumpp": "^8.2.1",
    "eslint": "^8.21.0",
    "esno": "^0.16.3",
    "pnpm": "^7.8.0",
    "react": "^18.2.0",
    "rimraf": "^3.0.2",
    "tsup": "^6.2.1",
    "typescript": "^4.7.4",
    "vite": "^3.0.4",
    "vitest": "^0.20.2",
    "vue": "^3.2.37"
  }
